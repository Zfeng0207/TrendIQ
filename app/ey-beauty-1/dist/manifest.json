{
	"_version": "1.76.0",
	"sap.app": {
		"id": "beautydashboard.eybeauty1",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			]
		},
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "@sap/generator-fiori:ovp",
			"version": "1.19.5",
			"toolsId": "8dfd5a5d-e454-41ee-930e-da856199d6a2"
		},
		"dataSources": {
			"mainService": {
				"uri": "/lead/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			},
			"accountService": {
				"uri": "/account/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			},
			"opportunityService": {
				"uri": "/opportunity/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			},
			"activityService": {
				"uri": "/activity/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			},
			"productService": {
				"uri": "/product/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": true,
		"dependencies": {
			"minUI5Version": "1.142.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.fe.templates": {},
				"sap.ovp": {},
				"sap.ui.rta": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "beautydashboard.eybeauty1.i18n.i18n",
					"supportedLocales": [
						""
					]
				}
			},
			"mainModel": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"accountModel": {
				"dataSource": "accountService",
				"preload": true,
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"opportunityModel": {
				"dataSource": "opportunityService",
				"preload": true,
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"activityModel": {
				"dataSource": "activityService",
				"preload": true,
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"productModel": {
				"dataSource": "productService",
				"preload": true,
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties",
				"settings": {
					"supportedLocales": [
						""
					]
				}
			}
		},
		"resources": {
			"css": []
		},
		"routing": {
			"config": {},
			"routes": [],
			"targets": {}
		},
		"flexBundle": false
	},
	"sap.ovp": {
		"globalFilterModel": "mainModel",
		"globalFilterEntitySet": "Leads",
		"containerLayout": "resizable",
		"enableLiveFilter": true,
		"considerAnalyticalParameters": false,
		"cards": {
			"card01_LeadPipeline": {
				"model": "mainModel",
				"template": "sap.ovp.cards.charts.analytical",
				"settings": {
					"title": "Lead Pipeline by Status",
					"subTitle": "Distribution of leads across different stages",
					"entitySet": "Leads",
					"selectionAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#LeadsByStatus",
					"chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#LeadsByStatus",
					"dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#LeadCount",
					"identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification"
				}
			},
			"card02_LeadQuality": {
				"model": "mainModel",
				"template": "sap.ovp.cards.charts.analytical",
				"settings": {
					"title": "Lead Quality Distribution",
					"subTitle": "Hot, Warm, and Cold leads",
					"entitySet": "Leads",
					"selectionAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#LeadsByQuality",
					"chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#LeadsByQuality",
					"dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#LeadCount",
					"identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification"
				}
			},
			"card03_TopLeads": {
				"model": "mainModel",
				"template": "sap.ovp.cards.list",
				"settings": {
					"title": "Top Leads by AI Score",
					"subTitle": "Highest potential leads",
					"entitySet": "Leads",
					"listType": "condensed",
					"listFlavor": "standard",
					"sortBy": "aiScore",
					"sortOrder": "descending",
					"annotationPath": "com.sap.vocabularies.UI.v1.LineItem#TopLeads"
				}
			},
			"card04_AccountHealth": {
				"model": "accountModel",
				"template": "sap.ovp.cards.list",
				"settings": {
					"title": "Account Health Status",
					"subTitle": "Key customer accounts",
					"entitySet": "Accounts",
					"listType": "condensed",
					"listFlavor": "standard",
					"sortBy": "healthScore",
					"sortOrder": "descending",
					"annotationPath": "com.sap.vocabularies.UI.v1.LineItem#AccountHealth"
				}
			},
			"card05_SalesPipeline": {
				"model": "opportunityModel",
				"template": "sap.ovp.cards.charts.analytical",
				"settings": {
					"title": "Sales Pipeline by Stage",
					"subTitle": "Opportunities across sales stages",
					"entitySet": "Opportunities",
					"selectionAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#OpportunitiesByStage",
					"chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#OpportunitiesByStage",
					"dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#OpportunityRevenue",
					"identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification"
				}
			},
			"card06_RevenueForecast": {
				"model": "opportunityModel",
				"template": "sap.ovp.cards.list",
				"settings": {
					"title": "Revenue Forecast",
					"subTitle": "Top opportunities by expected revenue",
					"entitySet": "Opportunities",
					"listType": "condensed",
					"listFlavor": "standard",
					"sortBy": "expectedRevenue",
					"sortOrder": "descending",
					"annotationPath": "com.sap.vocabularies.UI.v1.LineItem#RevenueOpportunities"
				}
			},
			"card07_RecentActivities": {
				"model": "activityModel",
				"template": "sap.ovp.cards.table",
				"settings": {
					"title": "Recent Activities",
					"subTitle": "Latest customer interactions",
					"entitySet": "Activities",
					"sortBy": "startDateTime",
					"sortOrder": "descending",
					"annotationPath": "com.sap.vocabularies.UI.v1.LineItem#RecentActivities"
				}
			},
			"card08_TopProducts": {
				"model": "productModel",
				"template": "sap.ovp.cards.list",
				"settings": {
					"title": "Top Performing Products",
					"subTitle": "Products by trend score",
					"entitySet": "Products",
					"listType": "extended",
					"listFlavor": "standard",
					"sortBy": "trendScore",
					"sortOrder": "descending",
					"annotationPath": "com.sap.vocabularies.UI.v1.LineItem#TopProducts"
				}
			}
		},
		"globalFilterControlType": "MacroFilterBar"
	},
	"sap.fiori": {
		"registrationIds": [],
		"archeType": "analytical"
	}
}