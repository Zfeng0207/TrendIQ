{
  "_version": "1.59.0",
  "sap.app": {
    "id": "beautyleads.campaigns",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.14.4",
      "toolsId": "manual-creation"
    },
    "dataSources": {
      "mainService": {
        "uri": "/marketing/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "intent1": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "Campaigns",
          "action": "manage",
          "title": "{{appTitle}}",
          "icon": "sap-icon://marketing-campaign"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://marketing-campaign",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "beautyleads.campaigns.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/custom.css"
        },
        {
          "uri": "css/campaign-dashboard.css"
        }
      ]
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ObjectPage.ObjectPageController": {
            "controllerName": "beautyleads.campaigns.ext.controller.ObjectPageExt"
          }
        }
      }
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "MarketingCampaignsList",
          "target": "MarketingCampaignsList"
        },
        {
          "pattern": "MarketingCampaigns({key}):?query:",
          "name": "MarketingCampaignsObjectPage",
          "target": "MarketingCampaignsObjectPage"
        }
      ],
      "targets": {
        "MarketingCampaignsList": {
          "type": "Component",
          "id": "MarketingCampaignsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/MarketingCampaigns",
              "variantManagement": "None",
              "navigation": {
                "MarketingCampaigns": {
                  "detail": {
                    "route": "MarketingCampaignsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "enableExport": true,
                    "selectAll": true,
                    "growingThreshold": 15,
                    "growing": true,
                    "growingScrollToLoad": true
                  }
                }
              },
              "initialLoad": true
            }
          }
        },
        "MarketingCampaignsObjectPage": {
          "type": "Component",
          "id": "MarketingCampaignsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": true,
              "contextPath": "/MarketingCampaigns",
              "content": {
                "header": {
                  "visible": true,
                  "anchorBarVisible": true
                },
                "body": {
                  "sections": {
                    "CampaignDashboard": {
                      "template": "beautyleads.campaigns.ext.fragments.CampaignDashboard",
                      "position": {
                        "placement": "Before",
                        "anchor": "CampaignDetails"
                      },
                      "title": "Campaign Analytics"
                    }
                  }
                }
              },
              "navigation": {
                "creators": {
                  "detail": {
                    "route": "CampaignCreatorsObjectPage",
                    "navigationProperty": "creators"
                  }
                },
                "triggerProduct": {
                  "detail": {
                    "route": "ProductsObjectPage",
                    "navigationProperty": "triggerProduct"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "beautyleads"
  }
}

