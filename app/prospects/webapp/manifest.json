{
  "_version": "1.59.0",
  "sap.app": {
    "id": "beautyleads.prospects",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.14.4",
      "toolsId": "manual-creation"
    },
    "dataSources": {
      "mainService": {
        "uri": "/prospect/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "intent1": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "Prospects",
          "action": "manage",
          "title": "{{appTitle}}",
          "icon": "sap-icon://person-placeholder"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://person-placeholder",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "beautyleads.prospects.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/custom.css"
        }
      ]
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ProspectsList",
          "target": "ProspectsList"
        },
        {
          "pattern": "Prospects({key}):?query:",
          "name": "ProspectsObjectPage",
          "target": "ProspectsObjectPage"
        }
      ],
      "targets": {
        "ProspectsList": {
          "type": "Component",
          "id": "ProspectsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Prospects",
              "variantManagement": "None",
              "navigation": {
                "Prospects": {
                  "detail": {
                    "route": "ProspectsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "enableExport": true,
                    "selectAll": true,
                    "growingThreshold": 50,
                    "growing": true,
                    "growingScrollToLoad": true
                  }
                }
              },
              "initialLoad": true
            }
          },
          "extends": {
            "extensions": {
              "sap.ui.controllerExtensions": {
                "sap.fe.templates.ListReport.ListReportController": {
                  "controllerName": "beautyleads.prospects.ext.controller.ListReportExt"
                }
              }
            }
          }
        },
        "ProspectsObjectPage": {
          "type": "Component",
          "id": "ProspectsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": true,
              "contextPath": "/Prospects",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.Identification": {
                  "actions": {
                    "DataFieldForAction::ProspectService.initiateAIMeeting": {
                      "visible": false
                    },
                    "DataFieldForAction::ProspectService.assignToSalesRep": {
                      "press": "beautyleads.prospects.ext.actions.AssignSalesRepAction.onAssignToSalesRep"
                    },
                    "DataFieldForAction::ProspectService.createOpportunity": {
                      "visible": false
                    },
                    "DataFieldForAction::ProspectService.convertToAccount": {
                      "visible": false
                    },
                    "CustomConvertProspect": {
                      "press": "beautyleads.prospects.ext.actions.ConvertProspectAction.onConvertProspect",
                      "text": "Convert",
                      "visible": true,
                      "enabled": true
                    },
                    "CustomAIMeeting": {
                      "press": "beautyleads.prospects.ext.actions.AIMeetingActions.onAIMeetingInitiator",
                      "text": "AI Meeting Initiator",
                      "visible": true,
                      "enabled": true
                    }
                  }
                }
              },
              "content": {
                "header": {
                  "actions": {
                    "CustomConvertProspectHeader": {
                      "press": "beautyleads.prospects.ext.actions.ConvertProspectAction.onConvertProspect",
                      "text": "Convert",
                      "visible": true,
                      "enabled": true
                    },
                    "CustomAIMeetingHeader": {
                      "press": "beautyleads.prospects.ext.actions.AIMeetingActions.onAIMeetingInitiator",
                      "text": "AI Meeting Initiator",
                      "visible": true,
                      "enabled": true,
                      "type": "Emphasized"
                    }
                  }
                }
              }
            }
          },
          "extends": {
            "extensions": {
              "sap.ui.controllerExtensions": {
                "sap.fe.templates.ObjectPage.ObjectPageController": {
                  "controllerName": "beautyleads.prospects.ext.controller.ProspectsCreatioExt"
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "beautyleads"
  }
}

